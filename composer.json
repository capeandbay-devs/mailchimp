{
    "name": "capeandbay/mailchimp",
    "description": "Mailchimp API wrapper, includes Laravel 6/7 support.",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim",
            "email": "tim@nztim.com"
        },
        {
            "name": "Angel Gonzalez",
            "email": "angel@capeandbay.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "rmccue/requests": "^1.7"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "CapeAndBay\\Mailchimp\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CapeAndBay\\Mailchimp\\MailchimpServiceProvider"
            ],
            "aliases": {
                "Mailchimp": "CapeAndBay\\Mailchimp\\MailchimpFacade"
            }
        }
    }
}
